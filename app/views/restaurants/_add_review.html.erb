<%= form_for(@review) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="field">
    <div><%= f.hidden_field :restaurant_id %></div>
    <%= f.text_area :user_feedback, placeholder: "Add review ...." %>
  </div>
  <%= f.submit "Post", class: "btn btn-large btn-primary" %>
<% end %>

